        <arguments>
          <argument>
            <kind>1</kind>
            <string>
// MOVEMENT

horizontal_speed = 0; // velocidad horizontal
vertical_speed = 0; // velocidad vertical
grav = 3; // fuerza de gravedad
movespeed = 15; // velocidad de movimiento
jumpspeed = 35; // velocidad de salto

// SHOOTING
lenX = 40;
lenY = 55;
current_frame = 4;
cadency = 3;

// SHOP
count = 0;
timeShieldSeconds = 30;
timeShield = (60*timeShieldSeconds)/2;

isBuyingGhost = false;
isBuyingShield = false;

hasShieldActivated = false;
hasGhostActivated = false;
hasUmbrellaActivated = false;
hasFireballActivated = false;
numFireballShots = 20;

hasShield = false;
hasGhost = false;
hasUmbrella = false;
hasFireball = true;

// ENEMIES

new_bees = 2;
live = 100;
injured = false;

xHud = 70;
yHud = 70;

// ECONOMY
global.roundWinner = 0;
global.player1_coins = 0;
global.player1_totalcoins = 0;
global.player1_victories = 0;
global.numBees = 0;
global.numHoneyComb = 1;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// CHARACTER CONTROLLER
//show_debug_message(hasFireballActivated);

/// GRAVEDAD 

// Añadimos la gravedad
if (vertical_speed &lt; 20 &amp;&amp; (!place_meeting(x,y+1,ground_obj) )){
    vertical_speed += grav;
}

// INPUT 

    // move
    key_right = keyboard_check(ord('D'));
    key_left = -keyboard_check(ord('A'));
    move = key_left + key_right;
    
    horizontal_speed = move * movespeed;
    
    // Shoot
    //key_shoot = keyboard_check_pressed(vk_lshift);
    key_shoot = keyboard_check(ord('C'));
    
    //Aim up
    key_aim_up = keyboard_check(ord('W'));
    
    // jump
    key_jump = keyboard_check(ord('X'));
    
    //Power Up
    key_powerup = keyboard_check(ord("E"));
     
    
    // QUIT
     key_esc = keyboard_check(vk_escape);
// JUMP

if(place_meeting(x,y+1,ground_obj) &amp;&amp; key_jump)
{
    var snd = audio_play_sound(snd_jump, 1, false);
    
    vertical_speed = -jumpspeed;
}

// USE ITEMS


 if(key_powerup &amp;&amp; hasGhost &amp;&amp; global.state == 'inGame'){
    audio_play_sound(snd_power_up, 1, false);
    hasGhost = false;
    // activate ghost
    hasGhostActivated = true;
    instance_create(x,y,ghost_obj);
    ghost_obj.player = 1;
    show_debug_message("activate ghost");
 }
 
 if(key_powerup &amp;&amp; hasShield &amp;&amp; global.state == 'inGame')
 {
    audio_play_sound(snd_power_up, 1, false);
    hasShield = false;
    hasShieldActivated = true;
    // activate shield
    show_debug_message("activate shield");
 }
 
 if(hasUmbrella)
 {
    hasUmbrella = false;
    hasUmbrellaActivated = true;
    instance_create(0,0,umbrellaActive_obj);
    show_debug_message("activate Umbrella");
    
 }
 
 
 // DISPARO

 if(key_shoot &amp;&amp; current_frame &gt;= cadency){
    current_frame = 0;
    if(hasFireballActivated){
        //Si tiene la bola de fuego activada
        //show_debug_message("Activadooooo!");
        numFireballShots = numFireballShots - 1;
        //show_debug_message(numFireballShots);
        if(numFireballShots == 0){
            numFireballShots = 20;
            hasFireballActivated = false;
        }
        if(key_aim_up){
            
            
            var bullet_id;
    
            bullet_id = instance_create(x,y + lenY-50,fireballShot_obj);
            bullet_id.direction = 90;
            bullet_id.player = 1;
    
            bullet_id.speed = 25;
            keyboard_clear(ord("C"));
            //keyboard_clear(ord("W"));
        }else{
            var bullet_id;
            if(image_xscale == 1){
                bullet_id = instance_create(x + lenX + 50,y + lenY,fireballShot_obj);
                bullet_id.direction = 0;
                bullet_id.player = 1;
            }else{
                bullet_id = instance_create(x - lenX - 5 - 50,y + lenY,fireballShot_obj);
                bullet_id.direction = 180;
                bullet_id.player = 1;
            }
            bullet_id.speed = 25;
            keyboard_clear(ord("C"));
        }
        
    }else{
        if(key_aim_up){
            var bullet_id;
    
            bullet_id = instance_create(x,y + lenY,purpleShot_obj);
            bullet_id.direction = 90;
    
            bullet_id.speed = 25;
            keyboard_clear(ord("C"));
            //keyboard_clear(ord("W"));
        }else{
            var bullet_id;
            if(image_xscale == 1){
                bullet_id = instance_create(x + lenX,y + lenY,purpleShot_obj);
                bullet_id.direction = 0;
            }else{
                bullet_id = instance_create(x - lenX - 5,y + lenY,purpleShot_obj);
                bullet_id.direction = 180;
            }
            bullet_id.speed = 25;
            keyboard_clear(ord("C"));
        }
        
    }
 
 }else if(current_frame &lt; cadency){
    current_frame++;
 }


 

// COLISIONES

// Colision vertical contra ground_obj
// si a la siguiente iteración toca al suelo
if(place_meeting(x,y+vertical_speed,ground_obj) )
{
    
    // recorrer uno a uno los pixeles de bajada para no pasarse
    while(!place_meeting(x,y+sign(vertical_speed),ground_obj))
    {
        y += sign(vertical_speed);
    }
    vertical_speed = 0;
}

// Colision horizontal
if(place_meeting(x+horizontal_speed,y,ground_obj))
{
    // recorrer uno a uno los pixeles de bajada para no pasarse
    while(!place_meeting(x+sign(horizontal_speed),y,ground_obj))
    {
        x += sign(horizontal_speed);
    }
    horizontal_speed = 0;

}







// animate 
if(move !=0){
    image_xscale = move; // invert the animations depending of the direction
    
} 
if(place_meeting(x,y+1,ground_obj))
{
    if(hasShieldActivated){
    
        if(move !=0){
           if(key_aim_up)
            sprite_index = spr_player1_runFire1; 
           else
            sprite_index = spr_player1_run1; 
        }else{
            if(key_aim_up)
                sprite_index  = spr_player1_fireUp1;
            else
                sprite_index = spr_player1_idle1;
        }
    }else{
        if(move !=0){
         if(key_aim_up)
            sprite_index = spr_player1_runFire; 
         else
            sprite_index = spr_player1_run; 
        }else{
            if(key_aim_up)
                sprite_index  = spr_player1_fireUp;
            else
                sprite_index = spr_player1_idle;
        }
    }
        
        
}else
{
    if(hasShieldActivated) 
        if(vertical_speed &lt; 0 &amp;&amp; hasShieldActivated) sprite_index = spr_player1_jump1; else sprite_index = spr_player1_fall1;
    else
        if(vertical_speed &lt; 0 &amp;&amp; !hasShieldActivated) sprite_index = spr_player1_jump; else sprite_index = spr_player1_fall; 

}


// sumamos la gravedad al personaje
y += vertical_speed;
x += horizontal_speed;

if(hasShieldActivated)
{
    count++;
    
    if(count == timeShield)
    {
        count =0;
        hasShieldActivated = false;
    }
}

// GAME OVER 
if(live &lt;= 0){
    global.roundWinner = 2;
  // var numb = asset_get_index("room"+string(3));
    //room_goto(tienda);
    gameSystem_obj.done = true;
    live= 100;
    player2_obj.live = 100;
}

if(key_esc)
{
    room_goto(mainMenu);

}

if(injured)
{
    sprite_index = spr_player1_Injured;
    //audio_play_sound(snd_hurt, 1, false);
    injured = false;

}




</string>
          </argument>
        </arguments>
      </action>
