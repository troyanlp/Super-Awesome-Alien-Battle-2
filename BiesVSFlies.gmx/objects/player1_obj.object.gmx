          <argument>
            <kind>1</kind>
            <string>if(global.inGame)
{
    // GUI
    
    draw_sprite(spr_player1_hud,0,xHud-30,yHud-20);
    
    // Background
    draw_sprite(spr_healthbar_bg,1,xHud+31,yHud+20);
    // Inner
    draw_sprite_ext(spr_healthbar_inner,1,xHud+31,yHud+20,live/100,1,0,c_white,1);
    // Border
    draw_sprite(spr_healthbar_border,1,xHud,yHud);
    
    // Coins
    draw_sprite(spr_coin,1,xHud-30,yHud+30);
    draw_set_font(fnt_HUD);
    draw_set_colour(c_white);
    // coin number
    draw_text(xHud+80,yHud+50,global.player1_coins);
    
    // POWERS !!!
    
    // Ghost
    if(player1_obj.hasGhost)draw_sprite(spr_ghost_hud,0,xHud+50,yHud-30);
    
    // Shield
    if(player1_obj.hasShield)draw_sprite(spr_shield_hud,0,xHud+80,yHud-35);
    
    // Fireball
    if(player1_obj.hasFireballActivated)draw_sprite(spr_fireball_hud,0,xHud+110,yHud-45);
    
    // Umbrella
    if(player1_obj.hasUmbrellaActivated)draw_sprite(spr_umbrella_hud,0,xHud+170,yHud-30);
    
    
}

    
</string>
          </argument>
        </arguments>
      </action>
