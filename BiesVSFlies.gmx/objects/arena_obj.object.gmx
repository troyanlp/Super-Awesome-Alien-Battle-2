if keyboard_check_pressed(vk_enter){
    switch(button){
        case 0:
            room_goto(mainMenu);
            break;
        case 1:
            //3 rondas
            global.simple = false;
            global.numRound = 3;
            global.inGame = true;
            audio_stop_sound(snd_Nivel7);
            global.currentMusic = snd_tienda;
            audio_play_sound(global.currentMusic, 1, true);
            room_goto(tienda);
            break;
        case 2:
            //5 rondas
            global.simple = false;
            global.numRound = 5;
            global.inGame = true;
            audio_stop_sound(snd_Nivel7);
            global.currentMusic = snd_tienda;
            audio_play_sound(global.currentMusic, 1, true);
            room_goto(tienda);
            break;
        case 3:
            //7 rondas
            global.simple = false;
            global.numRound = 7;
            global.inGame = true;
            audio_stop_sound(snd_Nivel7);
            global.currentMusic = snd_tienda;
            audio_play_sound(global.currentMusic, 1, true);
            room_goto(tienda);
            break;
    }
}


