            <kind>1</kind>
            <string>if keyboard_check_pressed(vk_down){
    if(button &lt; 4){
        button++;
    }else{
        button = 1;
    }
}

if keyboard_check_pressed(vk_up){
    if(button &gt; 1){
        button--;
    }else{
        button = 4;
    }
}

if keyboard_check_pressed(vk_enter){
    switch(button){
        case 1:
            //Llevar a la partida rápida
            //room_goto(room1);
            global.simple = true;
            global.numRound = 1;
            global.inGame = true;
            audio_stop_sound(snd_Nivel7);
            global.currentMusic = snd_tienda;
            audio_play_sound(global.currentMusic, 1, true);
            room_goto(tienda);
            break;
        case 2:
            //Llevar al menú de Arena
            room_goto(arenaMenu);
            break;
        case 3:
            //Llevar al menú de controles
            room_goto(controlsMenu);
            break;
    }
}


switch(button){
    case 1:
        btn_menuPlay_obj.selected = true;
        btn_menuArena_obj.selected = false;
        btn_menuControls_obj.selected = false;
        btn_menuCredits_obj.selected = false;
        credits_obj.visible = false;
        if(audio_is_playing(snd_Epic_Sax_Guy)){
            global.currentMusic = snd_Nivel7;
            audio_stop_sound(snd_Epic_Sax_Guy);
            audio_play_sound(global.currentMusic, 1, true);
        }
        break;
    case 2:
        btn_menuPlay_obj.selected = false;
        btn_menuArena_obj.selected = true;
        btn_menuControls_obj.selected = false;
        btn_menuCredits_obj.selected = false;
        credits_obj.visible = false;
        if(audio_is_playing(snd_Epic_Sax_Guy)){
            global.currentMusic = snd_Nivel7;
            audio_stop_sound(snd_Epic_Sax_Guy);
            audio_play_sound(global.currentMusic, 1, true);
        }
        break;
    case 3:
        btn_menuPlay_obj.selected = false;
        btn_menuArena_obj.selected = false;
        btn_menuControls_obj.selected = true;
        btn_menuCredits_obj.selected = false;
        credits_obj.visible = false;
        if(audio_is_playing(snd_Epic_Sax_Guy)){
            global.currentMusic = snd_Nivel7;
            audio_stop_sound(snd_Epic_Sax_Guy);
            audio_play_sound(global.currentMusic, 1, true);
        }
        break;
    case 4:
        credits_obj.visible = true;
        btn_menuCredits_obj.selected = true;
        btn_menuPlay_obj.selected = false;
        btn_menuArena_obj.selected = false;
        btn_menuControls_obj.selected = false;
        audio_stop_sound(snd_Nivel7);
        global.currentMusic = snd_Epic_Sax_Guy;
        if(!audio_is_playing(snd_Epic_Sax_Guy)){
            
            audio_play_sound(global.currentMusic, 1, true);
        }
        
    break;
}
